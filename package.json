{
  "name": "eden-dusk",
  "displayName": "Eden Dusk",
  "description": "A dark theme that blends twilight hues and garden greens.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffcalaway/eden-dusk-theme.git"
  },
  "icon": "icon.png",
  "publisher": "jeffcalaway",
  "version": "1.3.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Eden Dusk",
        "uiTheme": "vs-dark",
        "path": "./themes/Eden Dusk-color-theme.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "scss.injection",
        "injectTo": ["source.css.scss"],
        "path": "./syntaxes/scss.tmLanguage.json"
      },
      {
        "scopeName": "php.injection",
        "injectTo": ["text.html.php"],
        "path": "./syntaxes/php.tmLanguage.json"
      }
    ]
  }
}
